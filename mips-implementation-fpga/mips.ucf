#NET "reset0" LOC = "K18" | IOSTANDARD = LVCMOS33 | PULLDOWN ;
#NET "reset1" LOC = "K17" | IOSTANDARD = LVCMOS33 | PULLDOWN ;
NET "reset0"                LOC = F18     | IOSTANDARD = LVCMOS33 | DRIVE = 8 | SLEW = FAST | PULLUP;  #DP 2
NET "reset1"                LOC = F17     | IOSTANDARD = LVCMOS33 | DRIVE = 8 | SLEW = FAST | PULLUP;  #DP 1

## líneas de control del LCD
